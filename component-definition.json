                      "fd:viewType": "typeahead",
                      "fieldType": "text-input",
                      "name": "course",
                      "jcr:title": "Course, qualification or certification",
                      "sling:resourceType": "core/fd/components/form/textinput/v1/textinput",
                      "datasource": "courses",
                      "required": true
                    },
